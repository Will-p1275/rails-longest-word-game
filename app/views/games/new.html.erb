<ul>
  <% @letters.each do |letter| %>
   <li><%= letter %></li>
  <% end %>
</ul>
<form action="/score" data-turbo="false" method="post">
    <input type="text" name="new">
    <input type="submit" value="New letter">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
</form>
